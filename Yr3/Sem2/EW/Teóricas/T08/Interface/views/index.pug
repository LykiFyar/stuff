extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Lista de EMD
    .w3-container
      table.w3-table-all
        tr
          th Data
          th Nome
          th Género
          th Idade
          th Resultado 
        each e in exams
            tr
              td= e.dataEMD
              td #[a(href="/" + e._id) #{e.nome.primeiro} #{e.nome.último}]
              td= e.género
              td= e.idade 
              td= e.resultado
          
    footer.w3-container.w3-teal
      h5 Generated by EMD_Aferição::EngWeb2023 in #{d}